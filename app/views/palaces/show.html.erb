<%= render 'shared/navbar' %>
<div class="content-wrapper">
    <div class="image-background-show">
    </div>
</div>
<div class='container'>
<div class= 'titre'>

<h2 class = "text-start"> <%= @palace.name %></h2>
<% if @palace.user != current_user %>
    <div class="booking-form-container">
      <strong><p class = 'text-align'><%= @palace.price %>€</strong> par nuit</p>
      <%= simple_form_for [@palace, @booking],
        html: {data: { controller: 'flatpickr' } } do |f| %>
        <%= f.input :begin_date, as: :string, input_html: { data: { flatpickr_target: 'startDateInput', } } %>
        <%= f.input :end_date, as: :string, input_html: { data: { flatpickr_target: 'endDateInput' } } %>
        <%= f.submit "Book!", class: "button-card" %>
      <% end %>
    </div>
<% end %>
<p class = 'text-bg-primary" style="width: 6rem;'> <em>The address of the palace is <%= @palace.address %></em></p>

<br>
<p class = 'text-align'> <%= @palace.description %></p>
